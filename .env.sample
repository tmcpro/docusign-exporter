# DocuSign Authentication
DOCUSIGN_TOKEN=eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1...  # Bearer token from the authorization header WITHOUT Bearer: part
DOCUSIGN_ACCOUNT_ID=79a32329-5f6c-41ce-ad7e.....  # Account ID from the URL
DOCUSIGN_USER_ID=2211d58b-6031-472e-9712.....  # User ID from the request
DOCUSIGN_COOKIE=your_cookie_value_here  # Cookie value from the request headers from website (not API)

# Optional Configuration
DOCUSIGN_ENVIRONMENT=production  # or 'demo'
DOCUSIGN_OUTPUT_PATH=./downloads  # Default download directory
DOCUSIGN_MAX_CONCURRENT=3  # Number of concurrent downloads (max 5)
DOCUSIGN_RETRY_ATTEMPTS=3  # Number of retry attempts
DOCUSIGN_RETRY_DELAY=1000  # Initial retry delay in milliseconds
DOCUSIGN_RATE_LIMIT=5  # Requests per second